# https://EditorConfig.org
root = true

[*]
charset = utf-8
insert_final_newline = true
trim_trailing_whitespace = true

indent_style = tab
tab_width = 8
indent_size = 8

[CMakeLists.txt]
tab_width = 4
indent_style = space
[*.cmake]
tab_width = 4
indent_style = space

[*.py]
tab_width = 4
indent_style = space
